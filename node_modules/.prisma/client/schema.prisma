generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Email {
  id        Int      @id @default(autoincrement())
  emailId   String   @unique
  expense   String
  price     Float
  month     Int
  createdAt DateTime @default(now())
}

model OAuthToken {
  id           String   @id @default(uuid())
  accessToken  String
  refreshToken String
  expiryDate   DateTime
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
